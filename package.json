{
  "name": "@mobilabsolutions/eslint-config",
  "version": "1.0.0",
  "description": "eslint configuration for Mobilab Solutions projects",
  "scripts": {
    "test": "eslint ./rules es5.js es6.js",
    "clean": "rm -rf node_modules",
    "release": "lerna publish --conventional-commits",
    "bootstrap": "lerna bootstrap",
    "updtr": "updtr --save-exact",
    "clean-all": "lerna run clean",
    "test-all": "lerna run test",
    "update-all": "lerna run updtr"
  },
  "author": "Jo√£o Henriques <joao.henriques@mobilabsolutions.com>",
  "license": "MIT",
  "dependencies": {
    "babel-eslint": "8.2.5",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-react": "7.10.0"
  },
  "devDependencies": {
    "eslint": "5.1.0",
    "lerna": "2.11.0",
    "prettier": "1.13.7",
    "updtr": "2.0.0"
  },
  "peerDependencies": {
    "eslint": "^4.19.1 || ^5.0.1",
    "prettier": "^1.13.7"
  },
  "keywords": [
    "code checker",
    "code linter",
    "code standards",
    "code style",
    "eslint-config",
    "eslint",
    "eslintconfig",
    "lint",
    "style checker",
    "style linter"
  ]
}
