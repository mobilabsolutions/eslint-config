{
  "lerna": "2.11.0",
  "packages": [
    "addons/*"
  ],
  "command": {
    "publish": {
      "allowBranch": "master",
      "bump": "prerelease",
      "conventionalCommits": true,
      "gitRemote": "upstream",
      "preid": "rc",
      "message": "chore(release): publish %s"
    }
  },
  "npmClient": "yarn",
  "npmClientArgs": [
    "--production",
    "--no-optional"
  ],
  "version": "independent",
  "independent": true
}